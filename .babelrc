{
    "presets": [
        [
            "@babel/preset-env",
            {
                "modules": false,
                "targets": {
                  "esmodules": true
                }
            }
        ],
        "@babel/preset-react"
    ],
    "plugins": [
        "styled-components",
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-syntax-dynamic-import"
    ]
}
